#ORG 3000H
#BEGIN 3000H



MVI B , 05H
MVI C ,09H;
MOV A,B;
ANA C;
STA 3100H;


MVI B , 05H
MVI C ,09H;
MOV A,B;
ORA C;
STA 3101H


MVI B , 05H
MVI C ,09H;
MOV A,B;
XRA C;
STA 3102H
HLT

