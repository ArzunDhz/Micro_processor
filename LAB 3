#org 0000H
#begin 0000H
MVI C,00H;
MVI A,EFH;
MVI B,FCH
ADD B 
JC YES
STA 2050
HLT
YES: 	 INR C 
	 MOV D,A
	STA 2050
	HLT

______________________2______________________	

# ORG 0000H
# BEGIN 0000H
	   LHLD 2050
	   XCHG
	   LHLD 2052
	   DAD D
	   MOV A,M
	   JC CARRY
	   SHLD 3050

CARRY:	   INR C
	   SHLD 3050
	   HLT
